<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>QuantGate.API.Signals</RootNamespace>
    <Company>QuantGate Systems Inc</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>QuantGate_SignalsAPI</PackageId>
    <Authors>John Hollander</Authors>
    <Version>1.0.0-alpha</Version>
    <Description>The QuantGate Signal API delivers the signals and gauge data used to generate spectrums and other visual indicators for QuantGate products. The API can be integrated with both mobile applications and enterprise legacy trading platforms, and can be used with any market data feed. Requires a valid JWT token, as supplied by QuantGate Systems Inc.</Description>
    <Copyright>2021 QuantGate Systems Inc.</Copyright>
    <PackageProjectUrl>https://quantgatesystems.com/saas.html</PackageProjectUrl>
    <PackageReleaseNotes>Initial release for alpha testing.</PackageReleaseNotes>
	<LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.17.3" />
    <PackageReference Include="WebSocketSharp-netstandard" Version="1.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AccountAPI\AccountAPI.csproj" />
  </ItemGroup>

</Project>
